void forward(input_Layer input_layer, double X[]) //X de taille 8 (1.1,0.0,1.0,0.1)
{
  double Z2[16];
  double A2[16];
  double Z3[4]; // 1 par 4
  double YHAT[4]
  mul(X, input_layer.W1, 4, 2 , 4 , Z2);
  matrice_apply(sigmoid, Z2, 16, A2);
  mul(A2, input_layer.W2, 4, 4, 1, Z3);
  matrice_apply()
}

double sigmoid(double z)
{
  return 1/(1+exp(-z));
}

